arr = []
n = int(input("Enter the number of numbers to enter: "))
for i in range(n):
    num = float(input(f"Enter number {i+1}: "))
    arr.append(num)
largest = arr[0]
smallest = arr[0]
for num in arr:
    if num > largest:
        largest = num
    if num < smallest:
        smallest = num
print(f"Largest number: {largest}")
print(f"Smallest number: {smallest}")
